import sys

def parse(string):
        string_jumble, numbers = string.split('|')
        numbers = [int(arg) for arg in numbers.split()]
        for index in numbers:
            sys.stdout.write(string_jumble[index - 1])
        sys.stdout.write('\n')

        
if __name__ == "__main__":
    lines = open(sys.argv[1], 'r')
    for line in lines:
        if not line.isspace():
            parse(line)

